# Use the following guidelines when editing this file:
# * Use this file to install without user interaction. Save the file 
#   with the following name:  SilentInput.properties
# * Any error condition that leads to failure, such as an invalid
#   installation directory, generates a log file in the user home
#   directory. For example: /home/<user name>/silentErrorLog.log


# Set the absolute path and file name of the license key file:
LICENSE_KEY_LOC=<%=node[:infainstaller][:silentinput][:license_key_loc]%>

# Set the directory for Informatica 9.5.0:
USER_INSTALL_DIR=<%=node[:infainstaller][:silentinput][:user_install_dir]%>

# Set the installation type: 
# 0 -> Select this option to perform a fresh installation of 9.5.0.
# 1 -> Select this option to upgrade previous versions of Informatica 
#      products to version 9.5.0. 
INSTALL_TYPE=<%=node[:infainstaller][:silentinput][:install_type]%>


# If INSTALL_TPE=0 (install), set the connection type:
# 0 -> Use HTTP connection
# 1 -> Use HTTPS connection for secure communication between the
#      Administrator tool and the Service Manager.
HTTPS_ENABLED=<%=node[:infainstaller][:silentinput][:https_enabled]%>

# If HTTPS_ENABLED=1 (use HTTPS connection), specify whether to use a
# keystore file generated by the installer or use an existing keystore.
# To use a keystore file generated by the installer, set the following
# parameter to 1. Otherwise set it to 0.
DEFAULT_HTTPS_ENABLED=<%=node[:infainstaller][:silentinput][:default_https_enabled]%>
# To use an existing keystore file, set the following parameter to 1.
# Otherwise set it to 0. Note: If DEFAULT_HTTPS_ENABLED=1, set this
# parameter to 0.
CUSTOM_HTTPS_ENABLED=<%=node[:infainstaller][:silentinput][:custom_https_enabled]%>
		
# If CUSTOM_HTTPS_ENABLED=1 (use an existing keystore), set the 
# following parameters:	 
# Plain text password for the keystore file:
KSTORE_PSSWD=<%=node[:infainstaller][:silentinput][:kstore_psswd]%>
# Path and file name of the keystore file:
KSTORE_FILE_LOCATION=<%=node[:infainstaller][:silentinput][:kstore_file_location]%>
# Port number to use for secure communication between the Administrator 
# tool and the Service Manager:
HTTPS_PORT=<%=node[:infainstaller][:silentinput][:https_port]%>


# Specify whether to create or join an Informatica domain.
# Create an Informatica domain if you are installing for the first time 
# or if you are creating multiple domains. To create a domain, set the
# following parameter to 1. Otherwise set it to 0.
CREATE_DOMAIN=<%=node[:infainstaller][:silentinput][:create_domain]%>
# You can join an Informatica domain created during a previous
# installation on another node. To join a domain, set the following
# parameter to 1. Otherwise set it to 0.
JOIN_DOMAIN=<%=node[:infainstaller][:silentinput][:join_domain]%>

# Enable Transport Layer Security (TLS):
# Set this property to "true" incase you want to enable transport layer security for domain
# Set only if CREATE_DOMAIN=1 , this property will not be used in case of join domain as the node will 
# get added on the bases of domain security
SSL_ENABLED=<%=node[:infainstaller][:silentinput][:ssl_enabled]%>

# If JOIN_DOMAIN=1, set the type of node to create:
# 0 -> Configure the node as a worker node
# 1 -> Configure the node as a gateway node
SERVES_AS_GATEWAY=<%=node[:infainstaller][:silentinput][:serves_as_gateway]%>


# Configure the Informatica domain configuration database and set the
# database parameters. 
# Database type: (set to Oracle or MSSQLServer or DB2 or Sybase)
DB_TYPE=<%=node[:infainstaller][:silentinput][:db_type]%>
# Database user ID:
DB_UNAME=<%=node[:infainstaller][:silentinput][:db_uname]%>
# User password:
DB_PASSWD=<%=node[:infainstaller][:silentinput][:db_passwd]%>
 
# If DB_TYPE=MSSQLServer, set the following parameters:
# SQL Server schema name
SQLSERVER_SCHEMA_NAME=<%=node[:infainstaller][:silentinput][:sqlserver_schema_name]%>


# If you are installing on Windows, you can select trusted connection 
# to connect to the SQL Server database using the Windows credentials 
# of the current user account.Also select DB_CUSTOM_STRING_SELECTION=1 and
# provide the default valid connect string for sqlserver in DB_CUSTOM_STRING
# like jdbc:informatica:sqlserver://dbhostname:port;DataBaseName=databasename
#On UNIX, set this parameter to 0.
# 0 -> Do not use trusted connection. Connect using a SQL Server user 
#      account
# 1 -> Use trusted connection
TRUSTED_CONNECTION=<%=node[:infainstaller][:silentinput][:trusted_connection]%>
 	
# If DB_TYPE=DB2, set the following parameters:
# Tablespace name (must be a 32K tablespace)
DB2_TABLESPACE=<%=node[:infainstaller][:silentinput][:db2_tablespace]%>
 	
# Specify whether to use a JDBC URL or a custom connection string to 
# connect to the database.
# 0 -> Use a JDBC URL to connect to the database
# 1 -> Use a custom connection string to connect to the database
DB_CUSTOM_STRING_SELECTION=<%=node[:infainstaller][:silentinput][:db_custom_string_selection]%>

 
# If DB_CUSTOM_STRING_SELECTION=0 (use JDBC URL), set the following
# parameters:
# Database service name:
DB_SERVICENAME=<%=node[:infainstaller][:silentinput][:db_servicename]%>
# Database address:
DB_ADDRESS=<%=node[:infainstaller][:silentinput][:db_address]%>
# JDBC parameters:
ADVANCE_JDBC_PARAM=<%=node[:infainstaller][:silentinput][:advance_jdbc_param]%>
	
# If DB_CUSTOM_STRING_SELECTION=1 (use a custom connection string), set
# the connection string. The string must be a valid connection string.
# Also set DB_SERVICENAME and DB_ADDRESS to blank i.e DB_SERVICENAME= ,DB_ADDRESS=
DB_CUSTOM_STRING=<%=node[:infainstaller][:silentinput][:db_custom_string]%>


# Set the following parameters for the Informatica
# domain to create/join:
#Domain name:
DOMAIN_NAME=<%=node[:infainstaller][:silentinput][:domain_name]%>
#Host name:
DOMAIN_HOST_NAME=<%=node[:infainstaller][:silentinput][:domain_host_name]%>
#Node name:
NODE_NAME=<%=node[:infainstaller][:silentinput][:node_name]%>
#Domain port number:
DOMAIN_PORT=<%=node[:infainstaller][:silentinput][:domain_port]%>
#Domain user name:
DOMAIN_USER=<%=node[:infainstaller][:silentinput][:domain_user]%>
#Domain password:
DOMAIN_PSSWD=<%=node[:infainstaller][:silentinput][:domain_psswd]%>
#Confirm password:
DOMAIN_CNFRM_PSSWD=<%=node[:infainstaller][:silentinput][:domain_cnfrm_psswd]%>

# If JOIN_DOMAIN=1, set the following parameters for the Informatica 
# domain to join. 
# Node Name:
JOIN_NODE_NAME=<%=node[:infainstaller][:silentinput][:join_node_name]%>
# Host Name of the domain to join:
JOIN_HOST_NAME=<%=node[:infainstaller][:silentinput][:join_host_name]%>
# Port Number:
JOIN_DOMAIN_PORT=<%=node[:infainstaller][:silentinput][:join_domain_port]%>
 
# Specify whether to manually set the port numbers for the domain and
# node or use the default port numbers assigned by the installer.
# 0 -> Use the default ports assigned by the installer
# 1 -> Explicitly set the ports
ADVANCE_PORT_CONFIG=<%=node[:infainstaller][:silentinput][:advance_port_config]%>

 
#If ADVANCE_PORT_CONFIG=1, set the following parameters
#Minimum port number:
MIN_PORT=<%=node[:infainstaller][:silentinput][:min_port]%>
#Maximum port number:
MAX_PORT=<%=node[:infainstaller][:silentinput][:max_port]%>
#Service Manager port:
TOMCAT_PORT=<%=node[:infainstaller][:silentinput][:tomcat_port]%>
#Informatica Administrator port:
AC_PORT=<%=node[:infainstaller][:silentinput][:ac_port]%>
#Service Manager shutdown port:
SERVER_PORT=<%=node[:infainstaller][:silentinput][:server_port]%>
#Informatica Administrator Shutdown port:
AC_SHUTDWN_PORT=<%=node[:infainstaller][:silentinput][:ac_shutdwn_port]%>
